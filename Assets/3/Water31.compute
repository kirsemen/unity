#pragma kernel CSMain

struct Pixel {
	float2 pos;
	float2 vec;
	int inside;
};

RWStructuredBuffer<Pixel> date;
int size;
RWTexture2D<float4> tex;


[numthreads(32, 32, 1)]
void CSMain(uint3 id : SV_DispatchThreadID)
{
	int i = id.x + id.y * 4096;

	if ((int)i < size) {
		tex[date[i].pos] = 1;

	}
}
